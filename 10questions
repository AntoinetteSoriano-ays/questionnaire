<script>
function csubmit()
{
var aArray = [];
aArray[0] = document.getElementById("ans").value;
aArray[1] = document.getElementById("ans1").value;
aArray[2] = document.getElementById("ans2").value;
aArray[3] = document.getElementById("ans3").value;
aArray[4] = document.getElementById("ans4").value;
aArray[5] = document.getElementById("ans5").value
aArray[6] = document.getElementById("ans6").value;
aArray[7] = document.getElementById("ans7").value;
aArray[8] = document.getElementById("ans8").value;
aArray[9] = document.getElementById("ans9").value;

var ansArray =[];
ansArray[0] = "2";
ansArray[1] = "18";
ansArray[2] = "20";
ansArray[3] = "0.4";
ansArray[4] = "9";
ansArray[5] = "pi/3";
ansArray[6] = "0.057";
ansArray[7] = "2";
ansArray[8] = "106";
ansArray[9] = "2100";
var score = 0;
for (var i=0;i<10;i++)
{
    if (ansArray[i] == aArray[i])
    {
        score+=1;
    }
}
document.getElementById("score").value = score;
}
</script>
